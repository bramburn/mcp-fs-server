{
  "name": "qdrant-codesearch",
  "publisher": "icelabz",
  "repository": {
    "type": "git",
    "url": "https://github.com/icelabz/qdrant-codesearch.git"
  },
  "displayName": "Qdrant Code Search",
  "description": "Semantic code search using Qdrant and Ollama",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:qdrant.search.view"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "qdrant-sidebar",
          "title": "Qdrant Search",
          "icon": "$(search)"
        }
      ]
    },
    "views": {
      "qdrant-sidebar": [
        {
          "type": "webview",
          "id": "qdrant.search.view",
          "name": "Search",
          "icon": "$(search-view)"
        }
      ]
    },
    "commands": [
      {
        "command": "qdrant.index.start",
        "title": "Qdrant: Index Workspace"
      },
      {
        "command": "qdrant.openSettings",
        "title": "Qdrant: Open Settings"
      }
    ],
    "configuration": {
      "title": "Semantic Search",
      "properties": {
        "semanticSearch.trace": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug tracing for the extension."
        },
        "semanticSearch.indexingEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable background indexing."
        },
        "semanticSearch.indexingMaxFiles": {
          "type": "number",
          "default": 500,
          "description": "Maximum number of files to index."
        },
        "semanticSearch.indexingExcludePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/node_modules/**",
            "**/.git/**",
            "**/out/**",
            "**/dist/**",
            "**/build/**"
          ],
          "description": "Glob patterns to exclude from indexing."
        },
        "semanticSearch.indexingIncludeExtensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "ts",
            "js",
            "tsx",
            "jsx",
            "json",
            "md",
            "txt",
            "html",
            "css"
          ],
          "description": "File extensions to include in indexing."
        },
        "semanticSearch.activeVectorDb": {
          "type": "string",
          "enum": [
            "qdrant",
            "pinecone"
          ],
          "default": "qdrant",
          "description": "The vector database to use for storing embeddings."
        },
        "semanticSearch.qdrantUrl": {
          "type": "string",
          "default": "http://localhost:6333",
          "description": "The URL of the Qdrant server."
        },
        "semanticSearch.qdrantApiKey": {
          "type": "string",
          "default": "",
          "description": "The API key for Qdrant (if required)."
        },
        "semanticSearch.pineconeIndexName": {
          "type": "string",
          "default": "",
          "description": "The name of the Pinecone index."
        },
        "semanticSearch.pineconeHost": {
          "type": "string",
          "default": "",
          "description": "The host URL of the Pinecone index."
        },
        "semanticSearch.pineconeApiKey": {
          "type": "string",
          "default": "",
          "description": "The API key for Pinecone."
        },
        "semanticSearch.activeEmbeddingProvider": {
          "type": "string",
          "enum": [
            "ollama",
            "openai",
            "gemini"
          ],
          "default": "ollama",
          "description": "The embedding provider to use for generating embeddings."
        },
        "semanticSearch.ollamaBaseUrl": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "The base URL of the Ollama server."
        },
        "semanticSearch.ollamaModel": {
          "type": "string",
          "default": "nomic-embed-text",
          "description": "The Ollama model to use for embeddings."
        },
        "semanticSearch.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "The API key for OpenAI."
        },
        "semanticSearch.openaiModel": {
          "type": "string",
          "default": "text-embedding-3-small",
          "description": "The OpenAI model to use for embeddings."
        },
        "semanticSearch.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "The API key for Google Gemini."
        },
        "semanticSearch.geminiModel": {
          "type": "string",
          "default": "text-embedding-004",
          "description": "The Google Gemini model to use for embeddings."
        },
        "semanticSearch.indexName": {
          "type": "string",
          "default": "",
          "description": "The name of the index for your codebase."
        },
        "semanticSearch.embeddingDimension": {
          "type": "number",
          "default": 768,
          "description": "The dimension of the embeddings."
        },
        "semanticSearch.searchLimit": {
          "type": "number",
          "default": 10,
          "minimum": 5,
          "maximum": 100,
          "description": "The maximum number of search results to return."
        },
        "semanticSearch.searchThreshold": {
          "type": "number",
          "default": 0.7,
          "minimum": 0,
          "maximum": 1,
          "description": "The minimum similarity score for search results."
        },
        "semanticSearch.includeQueryInCopy": {
          "type": "boolean",
          "default": false,
          "description": "Whether to include the search query when copying results."
        },
        "semanticSearch.guidanceSearchLimit": {
          "type": "number",
          "default": 2,
          "minimum": 1,
          "maximum": 50,
          "description": "The maximum number of guidance results to return in search."
        },
        "semanticSearch.guidanceSearchThreshold": {
          "type": "number",
          "default": 0.6,
          "minimum": 0,
          "maximum": 1,
          "description": "The minimum similarity score for guidance search results."
        },
        "semanticSearch.indexInfoByRepo": {
          "type": "object",
          "default": {},
          "description": "Internal state of indexing for repositories.",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "build:rust": "node ./scripts/build-rust.js",
    "compile": "npm run build:rust && tsc -p ./tsconfig.json && node ./scripts/setup-resources.js && npm run build:webview",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:webview": "vite build --watch",
    "build:webview": "vite build",
    "package": "node ./scripts/package-vsix.js",
    "bump:minor": "npm version minor --no-git-tag-version",
    "release:minor": "npm run bump:minor && npm run package",
    "test": "vitest run",
    "testr": "vitest run --reporter=json --outputFile=./results.json; node ./scripts/filter-test-results.js",
    "test:watch": "vitest",
    "lint": "eslint src/webviews",
    "lint:fix": "eslint src/webviews --fix"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "^2.0.315",
    "@pinecone-database/pinecone": "^6.1.3",
    "@qdrant/js-client-rest": "^1.16.1",
    "minimatch": "^10.1.1",
    "posthog-node": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "web-tree-sitter": "^0.22.6",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/vscode": "^1.106.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^3.7.1",
    "concurrently": "^9.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "tslib": "^2.6.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.0.0",
    "vitest": "^1.6.1"
  }
}
