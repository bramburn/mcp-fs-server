{
  "name": "qdrant-codesearch",
  "publisher": "icelabz",
  "repository": {
    "type": "git",
    "url": "https://github.com/icelabz/qdrant-codesearch.git"
  },
  "displayName": "Qdrant Code Search",
  "description": "Semantic code search using Qdrant and Ollama",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:qdrant.search.view"
  ],
  "main": "./out/extension/src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "qdrant-sidebar",
          "title": "Qdrant Search",
          "icon": "$(search)"
        }
      ]
    },
    "views": {
      "qdrant-sidebar": [
        {
          "id": "qdrant.search.view",
          "name": "Search",
          "icon": "$(search-view)"
        }
      ]
    },
    "commands": [
      {
        "command": "qdrant.index.start",
        "title": "Qdrant: Index Workspace"
      },
      {
        "command": "qdrant.openSettings",
        "title": "Qdrant: Open Settings"
      }
    ],
    "configuration": {
      "title": "Qdrant Code Search",
      "properties": {
        "qdrant.search.trace": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug tracing"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.json && node ./scripts/setup-resources.js && npm run build:webview",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:webview": "vite build --watch",
    "build:webview": "vite build",
    "package": "node ./scripts/package-vsix.js",
    "bump:minor": "npm version minor --no-git-tag-version",
    "release:minor": "npm run bump:minor && npm run package",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/webviews",
    "lint:fix": "eslint src/webviews --fix"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.1",
    "lucide-react": "^0.462.0",
    "posthog-node": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "web-tree-sitter": "^0.22.6",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vscode/test-electron": "^2.4.0",
    "@vscode/vsce": "^2.24.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.48.0",
    "vite": "^6.0.0",
    "vitest": "^1.0.0"
  }
}
